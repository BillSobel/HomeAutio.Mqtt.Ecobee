{
  "ecobeeName": "default",
  "ecobeeAppKey": "blank",
  "refreshInterval": 180,
  "brokerIp": "localhost",
  "brokerPort": 1883,
  "brokerUsername": null,
  "brokerPassword": null,
  "Serilog": {
    "Enrich": [ "FromLogContext" ],
    "MinimumLevel": "Information",
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "RollingFile",
        "Args": {
          "pathFormat": "logs/HomeAutio.Mqtt.Ecobee.log",
          "retainedFileCountLimit": 31
        }
      }
    ]
  }
}
